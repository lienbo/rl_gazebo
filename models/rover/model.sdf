<?xml version='1.0'?>
<sdf version='1.4'>
    <model name="rover">
        <static>false</static>
        
        <link name='chassis'>
            <pose>-0.02 0 0.15 0 0 0</pose>
            <inertial>
                <mass>50</mass>
            </inertial>
            <collision name='chassis_collision'>
                <geometry>
                    <box>
                        <size>.8 .4 .16</size>
                    </box>
                </geometry>
            </collision>
            <visual name='chassis_visual'>
                <geometry>
                    <box>
                        <size>.8 .4 .16</size>
                    </box>
                </geometry>
            </visual>
        </link>


        <link name="rear_left_wheel">
            <pose>-0.3 0.24 0.1 0 1.5707 1.5707</pose>
            <inertial>
                <mass>1.5</mass>
            </inertial>
            <collision name="rear_left_wheel_collision">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="rear_left_wheel_visual">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </visual>
        </link>

        <joint type="revolute" name="rear_left_joint">
            <pose>-0.3 0.2 0.1 0 0 0</pose>
            <child>rear_left_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
        </joint>


        <link name="rear_right_wheel">
            <pose>-0.3 -0.24 0.1 0 1.5707 1.5707</pose>
            <inertial>
                <mass>1.5</mass>
            </inertial>
            <collision name="rear_right_wheel_collision">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="rear_right_wheel_visual">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </visual>
        </link>

        <joint type="revolute" name="rear_right_joint">
            <pose>-0.3 -0.2 0.1 0 0 0</pose>
            <child>rear_right_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 1 0</xyz>
            </axis>
        </joint>


        <link name="front_left_wheel">
            <pose>0.3 0.24 0.1 0 1.5707 1.5707</pose>
            <inertial>
                <mass>1.5</mass>
            </inertial>
            <collision name="front_left_wheel_collision">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="front_left_wheel_visual">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </visual>
        </link>

        <joint type="revolute" name="front_left_joint">
            <pose>0.3 0.2 0.1 0 0 0</pose>
            <child>front_left_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 1 1</xyz>
            </axis>
        </joint>


        <link name="front_right_wheel">
            <pose>0.3 -0.24 0.1 0 1.5707 1.5707</pose>
            <inertial>
                <mass>1.5</mass>
            </inertial>
            <collision name="front_right_wheel_collision">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </collision>
            <visual name="front_right_wheel_visual">
                <geometry>
                    <cylinder>
                        <radius>.2</radius>
                        <length>.07</length>
                    </cylinder>
                </geometry>
            </visual>
        </link>

        <joint type="revolute" name="front_right_joint">
            <pose>0.3 -0.2 0.1 0 0 0</pose>
            <child>front_right_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 1 1</xyz>
            </axis>
        </joint>

        <plugin name="CarinaPlugin" filename="libCarinaPlugin.so">
            <chassis_name>chassis</chassis_name>
        </plugin>

    </model>
</sdf>

